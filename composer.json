{
  "name": "medianova/laravel-accounting",
  "description": "Accounting package for laravel",
  "license": "mit",
  "authors": [
    {
      "name": "Umut Cetinkaya",
      "email": "umut.cetinkaya@medianova.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
    "quickbooks/v3-php-sdk": "^6.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "orchestra/testbench": "^v6.18.0"
  },
  "autoload": {
    "psr-4": {
      "Medianova\\LaravelAccounting\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Medianova\\LaravelAccounting\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Medianova\\LaravelAccounting\\LaravelAccountingServiceProvider"
      ],
      "aliases": {
        "Sms": "Accounting"
      }
    }
  }
}
